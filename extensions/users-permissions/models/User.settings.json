{
  "kind": "collectionType",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "fullname": {
      "type": "string"
    },
    "bio": {
      "type": "string"
    },
    "avatar": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "tokenVersion": {
      "type": "integer",
      "default": 1,
      "private": true
    },
    "expiredAt": {
      "type": "datetime"
    },
    "telegramId": {
      "type": "biginteger",
      "unique": true
    },
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "score": {
      "type": "biginteger",
      "default": "0"
    },
    "dailyScore": {
      "type": "biginteger",
      "default": "0"
    },
    "bestScore": {
      "type": "biginteger",
      "default": "0"
    },
    "tasks": {
      "type": "dynamiczone",
      "components": [
        "tasks.task"
      ]
    }
  }
}
